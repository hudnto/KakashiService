@model KakashiService.Web.ViewModel.ConfigurationVM
@{
    ViewBag.Title = "Read Service";
}
<h3>Read Service</h3>


<div class="panel panel-default">
    <div class="panel-heading">
        <h3 class="panel-title">Configurations</h3>
    </div>
    <div class="panel-body">
        <div class="row">
            <div class="col-sm-12">
                <div class="form-group">
                    <label class="control-label">Endpoint:</label>
                    <input id="Url" type="text" class="form-control" placeholder="Type your endpoint to clone"/> 
                </div>
            </div>
        </div>
    </div>
    <div class="panel-footer">
        <div class="row">
            <div class="col-sm-12">
                <div class="pull-right">
                    <input id="btnClone" type="button" class="btn btn-primary" value="Read!">
                </div>
            </div>
        </div>
    </div>
</div>
<div id="result">
</div>
@section scripts{
    <script src="~/Scripts/Registration/Index.js"></script>
}